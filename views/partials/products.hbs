<table class="table table-dark table-striped text-center">
    <tbody>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Foto</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img width="100px" src="{{this.thumbnail}}" alt=""></td>
        </tr>
        {{/each}}
    </tbody>
</table>